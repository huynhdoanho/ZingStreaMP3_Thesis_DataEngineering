create database zingstreamp3_realtime;

create schema zingstreamp3_realtime.zingstreamp3;


 CREATE TABLE zingstreamp3.auth_events (
            city VARCHAR(200),
            firstName VARCHAR(200),
            gender VARCHAR(200),
            itemInSession INT,
            lastName VARCHAR(200),
            lat DOUBLE PRECISION,
            level VARCHAR(200),
            lon DOUBLE PRECISION,
            registration BIGINT,
            sessionId INT,
            state VARCHAR(200),
            success BOOLEAN,
            ts TIMESTAMP,
            userAgent VARCHAR(MAX),
            userId BIGINT,
            zip VARCHAR(200),
            year INT,
            month INT,
            day INT,
            hour INT
        );



CREATE TABLE zingstreamp3.listen_events (
            artist VARCHAR(MAX),
            song VARCHAR(MAX),
            duration DOUBLE PRECISION,
            ts TIMESTAMP,
            sessionId INT,
            auth VARCHAR(200),
            level VARCHAR(200),
            itemInSession INT,
            city VARCHAR(200),
            zip VARCHAR(200),
            state VARCHAR(200),
            userAgent VARCHAR(MAX),
            lon DOUBLE PRECISION,
            lat DOUBLE PRECISION,
            userId BIGINT,
            lastName VARCHAR(200),
            firstName VARCHAR(200),
            gender VARCHAR(200),
            registration BIGINT,
            year INT,
            month INT,
            day INT,
            hour INT
        );


CREATE  TABLE zingstreamp3.status_change_events (
            artist VARCHAR(MAX),
            auth VARCHAR(200),
            city VARCHAR(200),
            duration DOUBLE PRECISION,
            firstName VARCHAR(200),
            gender VARCHAR(200),
            itemInSession INT,
            lastName VARCHAR(200),
            lat DOUBLE PRECISION,
            level VARCHAR(200),
            lon DOUBLE PRECISION,
            method VARCHAR(200),
            page VARCHAR(200),
            registration BIGINT,
            sessionId INT,
            song VARCHAR(MAX),
            state VARCHAR(200),
            status INT,
            success BOOLEAN,
            ts TIMESTAMP,
            userAgent VARCHAR(MAX),
            userId BIGINT,
            zip VARCHAR(200),
            year INT,
            month INT,
            day INT,
            hour INT
        );


CREATE  TABLE zingstreamp3.page_view_events (
            artist VARCHAR(MAX),
            auth VARCHAR(200),
            city VARCHAR(200),
            duration DOUBLE PRECISION,
            firstName VARCHAR(200),
            gender VARCHAR(200),
            itemInSession INT,
            lastName VARCHAR(200),
            lat DOUBLE PRECISION,
            level VARCHAR(200),
            lon DOUBLE PRECISION,
            method VARCHAR(200),
            page VARCHAR(300),
            registration BIGINT,
            sessionId INT,
            song VARCHAR(MAX),
            state VARCHAR(200),
            status INT,
            success BOOLEAN,
            ts TIMESTAMP,
            userAgent VARCHAR(MAX),
            userId BIGINT,
            zip VARCHAR(200),
            year INT,
            month INT,
            day INT,
            hour INT
        );